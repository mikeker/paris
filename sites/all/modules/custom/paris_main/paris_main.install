<?php
/**
 * @file
 * Bootstraps enabling/disabling modules after the initial site install.
 */

/**
 * Installs Disqus module.
 */
function paris_main_update_7001(&$sandbox) {
  module_enable(array('disqus'));
}

/**
 * Installs Google Analytics.
 */
function paris_main_update_7002(&$sandbox) {
  module_enable(array('google_analytics'));
}
